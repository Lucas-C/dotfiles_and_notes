{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "eslint.format.enable": true,
  "editor.tabSize": 2,
  "files.eol": "\n",
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.associations": {
    "*.hbs": "hbs",
    "*.yaml": "yaml"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "cmake.configureOnOpen": true,
  "remote.WSL.debug": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[hbs]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[java]": {
    "editor.tabSize": 4,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "never"
    },
    "editor.defaultFormatter": "redhat.java"
  },
  "java.format.enabled": true,
  "java.test.defaultConfig": "myConfiguration",
  "java.jdt.ls.java.home": "C:\\DevHome\\tools\\jdk-14.0.2",
  "java.configuration.runtimes": [
    {
      "name": "JDK-14",
      "path": "C:\\DevHome\\tools\\jdk-14.0.2",
      "default": true
    },
    {
      "name": "JDK-11",
      "path": "C:\\Program Files\\AdoptOpenJDK\\jdk-11.0.11.9-hotspot"
    },
    {
      "name": "JDK-8",
      "path": "C:\\DevHome\\tools\\jdk1.8.0_40"
    }
  ],
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSaveMode": "file"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": "explicit"
    }
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": "explicit"
    }
  },
  "terraform.codelens.referenceCount": true,
  "typescript.autoClosingTags": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": false,
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never",
      "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml",
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 1,
  "nsis.compiler.pathToMakensis": "C:\\DevHome\\tools\\NSIS\\makensis",
  "explorer.confirmDelete": false,
  "security.allowedUNCHosts": [
    "wsl.localhost"
  ],
  "[java][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "never"
    }
  },
  "[terraform][terraform-vars]": {
    "editor.codeActionsOnSave": {
      "source.formatAll.terraform": "explicit"
    }
  },
  "editor.mouseWheelZoom": true,
  "cSpell.language": "en,fr,fr-FR",
  "cSpell.enabledLanguageIds": [
    "markdown",
    "plaintext"
  ],
  "cSpell.userWords": [
    "antagoniser",
    "impropice",
    "impropices",
    "playtest",
    "playtests",
    "roleplay"
  ],
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "cSpell.dictionaryDefinitions": [],
  "cSpell.enableFiletypes": [
    "markdown"
  ],
  "cSpell.maxNumberOfProblems": 1000
}